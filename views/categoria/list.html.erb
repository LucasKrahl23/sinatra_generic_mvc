<table>
  <tr>
    <th>id</th>
    <th>Nome</th>
    <th></th>
  </tr>
  <% list.each do |model| %>
    <tr>
      <td><%= model.id %></td>
      <td><%= model.nome %></td>
      <td>
        <a href="/categoria/<%= model.id %>/edit">Editar</a>
        <a href="/categoria/<%= model.id %>/delete">Remover</a>
      </td>
    </tr>
  <% end %>
</table>
